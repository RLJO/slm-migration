<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Report Cashbox Line Coins QWEB-->
    <record id="action_report_slm_cashboxline_pdf" model="ir.actions.report">
        <field name="name">Report Cashbox Line Coins</field>
        <field name="model">cash.session</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">slm_report_lpl.report_slm_cashboxline_pdf</field>
        <field name="report_file">slm_report_lpl.report_slm_cashboxline_pdf</field>
        <field name="binding_model_id" ref="model_cash_session"/>
        <field name="binding_type">report</field>
    </record>


    <record id="action_report_slm_cashboxline_pdf_repot" model="report.paperformat">
	    <field name="name">Report CashboxLine Coins</field>
	    <field name="default" eval="True"/>
	    <field name="format">Letter</field>
	    <field name="page_height">0</field>
	    <field name="page_width">0</field>
	    <field name="orientation">Portrait</field>
	    <field name="margin_top">25</field>
	    <field name="margin_bottom">45</field>
	    <field name="margin_left">10</field>
	    <field name="margin_right">10</field>
	    <field name="header_line" eval="False"/>
	    <field name="header_spacing">40</field>
	    <field name="dpi">90</field>
	    <field name="report_ids" eval="[(4, ref('action_report_slm_cashboxline_pdf'))]"/>
	</record>
</odoo>
